{
	"manifest_version": 2,
	
	"name": "YubNubEx",
	"description": "Extends YubNub to allow multi-tab searching and local overrides",
	"version": "1.2",

	"omnibox": { "keyword" : "y" },
	"icons": {
		"16": "img/yubnubex16.png",
		"48": "img/yubnubex48.png",
		"128": "img/yubnubex128.png"
	},
	"permissions": [
		"tabs",
		"storage",
		"http://yubnub.org/"
	],
	"options_page" : "options.html",
	
	"background": {
		"page": "background.html"
	},
	
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}