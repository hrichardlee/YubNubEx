{
	"manifest_version": 2,
	
	"name": "YubNubEx",
	"description": "Allows for local overrides and multi-tab searching for YubNub",
	"version": "1.0",

	"omnibox": { "keyword" : "y" },
	"icons": {
		"16": "img/YubNubExLogo16.png",
		"48": "img/YubNubExLogo48.png",
		"128": "img/YubNubExLogo128.png"
	},
	"permissions": [
		"tabs",
		"storage",
		"http://yubnub.org/"
	],
	"options_page" : "options.html",
	
	"background": {
		"page": "background.html"
	},
	
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}